<script>    
	function addNewContact()
	{
		document.getElementById("secondaryContactTable").style.display="block";
		document.getElementById("btnAddScondaryContact").style.display="none";
	}
	function clickSource(sourceRadio) {
		if (sourceRadio.value == "Referral")	
		{
			$("#dialog_referal_info").modal();	
		}else if (sourceRadio.value == "Tradeshow")	
		{
			$("#dialog_tradeshows_info").modal();	
			
		}
	
	}
	$(document).ready(function() {
		/*$("#submit").hide();
		$("#submit_type").val("Normal");
