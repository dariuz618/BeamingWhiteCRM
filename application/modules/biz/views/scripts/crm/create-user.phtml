		 });
		 $("#submit_check").click(function() {
		 	
		 	// get apply value for selected
		 	var check_applys = document.getElementsByName('applys_check[]');
			var apply_vals = "";
			for (var i=0, n=check_applys.length;i<n;i++) 
			{
			    if (check_applys[i].checked) 
			    {
			        apply_vals += ","+check_applys[i].value;
			    }
			}
			if (apply_vals) 
			{
				apply_vals = apply_vals.substring(1);
				$("#apply").val(apply_vals);
			}

		 	var address1 = $('#address1').val();
		 	var address2 = $('#address2').val();
		 	var firstname = $('#firstname').val();
		 	var lastname = $('#lastname').val();
		 	var contactphone = $('#contactphone').val();
		 	var contactphone2 = $('#contactphone2').val();
		 	var email = $('#email').val();
		 	var email2 = $('#email2').val();
		 	
		 	
		 	$.ajax({
	            url: '/crm/search-account',
	            dataType: 'html',
	            type: "POST",
	            data: 'address1=' + address1 + '&address2=' + address2 + '&firstname=' + firstname + '&lastname=' + lastname + '&contactphone=' + contactphone + '&email=' + email+ '&contactphone2=' + contactphone2+ '&email2=' + email2,
	            success: function(res){
	            	console.log(res);
	                if (res == 'Yes')
	                {
	                	 $("#dialog_notify_duplicate").modal();
					}else
					{
						$("#submit").click();	
					}
	            }
	        });
	     });
